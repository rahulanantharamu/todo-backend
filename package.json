{
  "name": "todo-backend",
  "version": "1.0.0",
  "description": "A GraphQL Todo Backend API with TypeScript, PostgreSQL, and JWT Authentication",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:create": "npx sequelize-cli db:create"
  },
  "keywords": [
    "graphql",
    "typescript",
    "postgresql",
    "jwt",
    "todo"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^3.12.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.5.9",
    "@types/pg": "^8.10.2",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
